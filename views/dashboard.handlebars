{{#if posts.length}}
{{#each posts}}
  <div class="card border-light-dark">
    <div class="flex-row justify-space-between card-header bg-light-dark text-primary">
      <a href="/dashboard/edit/{{id}}"><h3>{{title}}</h3></a>
       <p>{{format_date created_at}}</p>
    </div>
  </div>
{{/each}}
{{/if}}